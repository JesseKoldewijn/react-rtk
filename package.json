{
  "name": "react-rtk",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n vite,routes-compile -c blue,green \"npm:dev:vite\" \"npm:compile:routes:watch\"",
    "dev:vite": "vite",
    "build": "node ./compile-routes.mjs && tsc && vite build",
    "preview": "vite preview",
    "compile:routes": "node ./compile-routes.mjs",
    "compile:routes:watch": "node --watch ./compile-routes.mjs",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "cssnano-preset-advanced": "^6.0.3",
    "postcss": "^8.4.33",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "volta": {
    "node": "20.11.0"
  }
}
